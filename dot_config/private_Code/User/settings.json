{
    "workbench.iconTheme": "vscode-icons",
    "window.titleBarStyle": "custom",
    "go.formatTool": "gofmt",
    "rust.mode": "rls",
    "rust.all_features": true,
    "rust.actionOnSave": "build",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "rust.rustup": {
        "toolchain": "nightly-x86_64-unknown-linux-gnu"
    },
    "python.languageServer": "Pylance",
    "workbench.panel.defaultLocation": "right",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "go.toolsManagement.autoUpdate": true,
    "go.goroot": "/home/yurinnick/.gvm/gos/go1.17",
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "/usr/bin/flatpak-spawn",
            "args": [
                "--host",
                "--env=TERM=xterm-256color",
                "bash"
            ]
        }
    },
    "remote.containers.dockerPath": "podman",
    "files.trimTrailingWhitespace": true,
    "cmake.configureOnOpen": true,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.python"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "yaml.schemas": {
        "/home/yurinnick/.vscode/extensions/continue.continue-1.0.10-linux-x64/config-yaml-schema.json": [
            ".continue/**/*.yaml"
        ]
    },
    "vs-kubernetes": {
        "vscode-kubernetes.minikube-path-linux": "/home/yurinnick/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vscode-kubernetes.helm-path-linux": "/home/yurinnick/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm"
    },
    "files.autoSave": "afterDelay"
}
